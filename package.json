{
  "name": "ewebinar-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "prestart": "npm run build",
    "watch": "tsc-watch --onSuccess \"node dist/index\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
    "start": "npm-run-all build watch",
    "start:prod": "node dist/index",
    "testdb:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run testdb:setup && jest",
    "test:watch": "npm run testdb:setup && jest --watch",
    "generate-schema": "ts-node ./src/utils/generateSchemaFile.ts",
    "serverless": "serverless offline --websocketPort 3002 --httpPort 3000 --lambdaPort 3001 --noAuth --noTimeout --printOutput"
  },
  "dependencies": {
    "@types/moment-timezone": "^0.5.12",
    "@types/node-fetch": "^2.5.5",
    "@types/rrule": "^2.2.9",
    "apollo-server-express": "^2.7.0",
    "aws-sdk": "^2.517.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "compact-timezone-list": "^1.0.6",
    "cookie-parser": "^1.4.4",
    "csv-stringify": "^5.3.6",
    "date-fns": "^2.9.0",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.0",
    "express-healthcheck": "^0.1.0",
    "graphql": "^14.4.2",
    "graphql-type-json": "^0.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.28",
    "morgan": "~1.9.0",
    "mysql": "^2.17.1",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "rrule": "^2.6.4",
    "shortid": "^2.2.15",
    "stripe": "^8.33.0",
    "twilio": "^3.39.5",
    "type-graphql": "^0.17.4",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.18",
    "typeorm-typedi-extensions": "^0.2.3",
    "uuid": "^3.3.3",
    "youtube-dl": "^2.0.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.2",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^24.0.16",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.6.8",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^3.4.5",
    "@types/youtube-dl": "^1.12.2",
    "coveralls": "^3.0.5",
    "eslint": "^6.8.0",
    "eslint-config-typescript": "^3.0.0",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "request": "^2.88.2",
    "rimraf": "^2.6.3",
    "serverless": "^1.65.0",
    "serverless-offline": "^6.0.0-alpha.67",
